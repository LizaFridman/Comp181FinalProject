+
+	CDR rbx
+	push rbx
+	
+	CAR rax
+	push rax
+
+	mov rax, 0
+	mov rdi, .space
+	call printf
+
+	call write_sob
+	add rsp, 1*8
+
+	call write_sob_pair_on_cdr
+	add rsp, 1*8
+
