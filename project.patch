+
+	mov rax, qword [rbp + 8 + 1*8]
+	mov rcx, rax
+	VECTOR_LENGTH rcx
+	cmp rcx, 0
+	je .done
+	VECTOR_ELEMENTS rax
+
+	push rcx
+	push rax
+	mov rax, qword [rax]
+	push qword [rax]
+	call write_sob
+	add rsp, 1*8
+	pop rax
+	pop rcx
+	dec rcx
+	add rax, 8
