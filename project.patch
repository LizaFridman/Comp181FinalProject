+	TYPE rbx
+	cmp rbx, T_NIL
+	je .done
+	
+	cmp rbx, T_PAIR
+	je .cdrIsPair
+	
+	push rax
+
+	mov rax, 0
+	mov rdi, .dot
+	call printf
+	
+	call write_sob
+	add rsp, 1*8
+
+	jmp .done
