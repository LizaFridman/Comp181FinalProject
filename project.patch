+(define pipeline
+  (lambda (sexpr)
+    ((star <sexpr>) sexpr
+     (lambda (match rest)
+       (map (lambda (expr)
+	      ;;(display (format "Pipelining ~a\n" expr))
+	      (annotate-tc
+	       (pe->lex-pe
+		(box-set
+		 (remove-applic-lambda-nil
+		  (parse expr))))))
+	    match))
+     (lambda (fail) 'fail))))
