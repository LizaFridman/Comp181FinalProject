+(define file->list
+  (lambda (in-file)
+    (let ((in-port (open-input-file in-file)))
+      (letrec ((run
+		(lambda ()
+		  (let ((readChar (read-char in-port)))
+		    (if (eof-object? readChar)
+			(begin
+			  (close-input-port in-port)
