+		     "cmp rdi, " (number->string lexical_env) "\n"
+		     "je "loop_copy_envs_end"\n"
+		     "mov rax, 8\n"
+		     "mul rdi\n"
+		     "cmp rdi, 999999 \n"
+		     "je " loop_copy_envs "\n" 
+		     "mov rcx, qword [r14+rax]\n" 
+		     "mov qword [rbx+rax+8], rcx\n" 
+		     "inc rdi\n"
+		     "jmp "loop_copy_envs"\n"		
+		     newLine     
+		     loop_copy_envs_end":\n"					
+		     "push rbx\n"
+		     "push rcx\n"
+		     "mov rdi, 16\n"
+		     "call malloc\n" 
+		     "pop rcx\n"
+		     "pop rbx\n"		     
+		     "MAKE_LITERAL_CLOSURE rax, rbx, " code "\n"
+		     "jmp "code_end"\n"
+		     newLine
+		     code":\n"
+		     "push rbp\n"
+		     "mov rbp, rsp\n"
+		     "mov rbx, " (sobNull) "\n"
+		     "mov r10, arg_count\n"
+		     newLine
+		     loop_fix_stack":\n"
+		     "cmp r10, "(number->string (length args)) "\n"
+		     "je " loop_fix_stack_end "\n"
+		     "mov rdi, 8\n"
+		     "call malloc\n"			
+		     "mov rdx, rbp\n"				
+		     "add rdx, 4*8\n"				
+		     "mov r11, r10\n"				
+		     "dec r11\n"
+		     "shl r11, 3\n"							
+		     "add rdx, r11\n"				
+		     "mov rdx, qword [rdx]\n"		
+		     "inc rax\n"
+		     "sub rax, 1\n"
