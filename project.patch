+(define string->sexprs
+  (lambda (str)
+    (let ((stringList (string->list str)))
+      (letrec ((translate
+                (lambda (lst)
+                  (<sexpr> lst 
+                           (lambda (expr rest) (if (null? rest)
+                                             (list expr)
+                                             (cons expr (translate rest))))
+                           (lambda (msg) `(error ,@msg))))))
+        (translate stringList)))))
